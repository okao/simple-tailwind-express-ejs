{
  "name": "tailwindsimple",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "nodemon --config nodemon.json server.js",
    "build-tail": "NODE_ENV=production tailwindcss build -i styles/tailwindcss.css -o assets/styles.css",
    "watch-tail": "NODE_ENV=development tailwindcss -i styles/tailwindcss.css -o assets/styles.css",
    "browser-sync": "browser-sync start",
    "dev": "concurrently \"npm run watch-tail\" \"npm run start\"",
    "prod": "npm run build-tail & npm run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "preline": "^2.0.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^2.29.3",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "tailwindcss-children": "^2.1.0"
  }
}
